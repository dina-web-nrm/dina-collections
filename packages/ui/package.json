    "mousetrap": "^1.6.2",
    "object-path": "^0.11.4",
    "object-path-immutable": "^1.0.1",
    "prettier": "1.16.4",
    "prop-types": "^15.6.0",
    "query-string": "5",
    "react": "^16.8.3",
    "react-app-polyfill": "^0.2.1",
    "react-dom": "^16.8.3",
    "react-json-inspector": "^7.1.1",
    "react-list": "^0.8.8",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "react-scripts": "2.1.5",
    "react-sortable-tree": "^2.1.0",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.3.0",
    "redux-module-local-storage": "^1.1.0",
