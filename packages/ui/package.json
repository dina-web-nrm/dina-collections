    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "github-markdown-css": "^2.10.0",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.19.1",
