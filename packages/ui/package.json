    "semantic-ui-react": "0.80.2",
    "watch-glob": "^0.1.3",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "yarn markdown:build && NODE_PATH=./src/ env-cmd ../../env/.ui react-scripts build",
    "build:trees": "node ./src/scripts/buildFileStructureTrees.js",
    "lint:js": "eslint '**/*.js'",
    "lint:js:fix": "eslint '**/*.js' --fix",
    "markdown:build": "node src/scripts/markdown/buildRenderedMarkdownFiles.js",
    "markdown:watch": "node src/scripts/markdown/watchMarkdownFiles.js",
    "pretty": "prettier --write 'src/**/*.{js,jsx,json}'",
    "start": "NODE_PATH=./src/ npm-run-all -p markdown:watch start-js",
    "start-js": "env-cmd ../../env/.ui react-scripts start",
    "test": "yarn lint:js && yarn test:unit",
    "test:coverage": "yarn test:unit --coverage",
    "test:depcheck": "depcheck . --skip-missing",
    "test:e2e": "yarn cypress run --browser chrome --record false",
    "test:e2e:ci:docker": "wait-on http://local-ui.dina-web.net && wait-on http://local-ui.dina-web.net/auth && yarn test:e2e --config baseUrl=http://local-ui.dina-web.net",
    "test:e2e:ci:local": "wait-on http://127.0.0.1:3000 && wait-on http://127.0.0.1:3000/auth && yarn test:e2e",
    "test:e2e:watch": "yarn cypress open",
    "test:unit": "env-cmd ../../env/.ui react-scripts test",
    "test:unit:fast": "REACT_APP_TEST_UI=false env-cmd ../../env/.ui react-scripts test --watch",
    "test:unit:watch": "env-cmd ../../env/.ui react-scripts test --watch",
    "update-git-dependencies": "yarn upgrade models && yarn upgrade semantic-ui"
  },
  "devDependencies": {
    "cypress": "^3.1.5",
    "deep-freeze": "^0.0.1",
    "depcheck": "^0.7.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest-localstorage-mock": "^2.0.1",
    "npm-run-all": "^4.1.1",
    "source-map-explorer": "^1.5.0",
    "wait-on": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "10.15.1",
    "yarn": "1.10.1"
  }
}
